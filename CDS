void setup()
{
  pinMode(LED_BUILTIN, OUTPUT);
  pinMode(13,OUTPUT);
  Serial.begin(9600);
}

void loop()
{
  int ad_input = analogRead(A0);
  
  Serial.print("ad_input");
  Serial.println(ad_input);

  delay(1000); // Wait for 1000 millisecond(s)
 
  if(ad_input < 300)
  {
    digitalWrite(LED_BUILTIN, HIGH);
  }
  
  else
  {
    digitalWrite(LED_BUILTIN,LOW);
  }
}
